{
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "data/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.py"
        }
    ]
}